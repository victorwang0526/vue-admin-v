    "element-ui": "^2.11.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-load-plugins": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "qs": "^6.7.0",
    "quill": "^1.3.6",
    "sass-loader": "^7.1.0",
